{
    "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
    "extends": "./api-extractor.json",
    "mainEntryPointFilePath": "<projectFolder>/temp/types/main.d.ts",
    "apiReport": {
        "reportFileName": "<unscopedPackageName>-main.api.md"
    },
    "dtsRollup": {
        "publicTrimmedFilePath": "<projectFolder>/dist/main.d.ts"
    },
    "messages": {
        "extractorMessageReporting": {
            "ae-missing-release-tag": {
                "logLevel": "none",
                "addToApiReportFile": true
            }
        },
        "tsdocMessageReporting": {
            "tsdoc-param-tag-missing-hyphen": {
                "logLevel": "none",
                "addToApiReportFile": true
            },
            "tsdoc-param-tag-with-invalid-type": {
                "logLevel": "none",
                "addToApiReportFile": true
            },
            "tsdoc-param-tag-with-invalid-optional-name": {
                "logLevel": "none",
                "addToApiReportFile": true
            },
            "tsdoc-escape-right-brace": {
                "logLevel": "none",
                "addToApiReportFile": true
            },
            "tsdoc-malformed-inline-tag": {
                "logLevel": "none",
                "addToApiReportFile": true
            }
        }
    }
}
